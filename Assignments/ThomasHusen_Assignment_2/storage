int length = lseek(fd, 0, SEEK_END);
printf("Length of file: %d\n\n", length);
numMove = lseek(fd, s, SEEK_SET);
numLeft = n;
numRead = 0;
// Forces standard output
fflush(stdout);

while(numLeft > 0 && numRead < length){
  if(numLeft <= 64){
    numRead = numRead + read(fd, outputBuffer, numLeft);
    write(1, outputBuffer, numLeft);
    numLeft = 0;
  }
  if(numLeft > 64){
    numRead = numRead + read(fd, outputBuffer, 64);
    write(1, outputBuffer, 64);
    printf("\n");
    numLeft = numLeft-64;
  }
}

// WORST CASE IS IT WRITES OUT A COMPLETE 64 CHARACTERS AT THE END then stops

if((numRead + 64) > length){
  numRead = numRead + read(fd, outputBuffer, length-numRead);
  write(1, outputBuffer, numLeft-numRead);
  printf("\n");
  numLeft = 0;
}
